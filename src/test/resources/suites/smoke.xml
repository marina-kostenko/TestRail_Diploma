<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Smoke test" parallel="tests" thread-count="2" >
    <test name="By groups">
        <parameter name="browserName" value="chrome"/>
        <groups>
            <run>
                <include name="smoke"/>
            </run>
        </groups>
        <packages>
            <package name="tests"/>
        </packages>
    </test>
</suite>
